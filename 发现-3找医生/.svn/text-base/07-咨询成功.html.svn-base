			<li class="mui-table-view-cell mui-media xy_myt">
				<a class="mui-navigate-right f16 font_blue">
					<img class="mui-media-object mui-pull-left" src="../img/logo.png">
						关注名医堂
				</a>
			</li>
		</ul>
		<script src="../js/mui.min.js"></script>
		<script type="text/javascript">
	    	function setH(){
				var vheight=window.screen.availHeight;
				var mheight=document.getElementsByClassName("mheight");
				mheight[0].style.minHeight=vheight+"px";
			}
			window.onload=function(){
				setH();
			}
			
	      	mui.init();
	    </script>
	</body>
